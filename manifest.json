{
  "description": "Opens salvage inventory sites in new tabs, searching for the copied VIN",
  "manifest_version": 2,
  "name": "Salvage Search",
  "version": "2.0",
  "homepage_url": "https://github.com/benetherington/salvage-search",
  "icons": {
    "48": "icons/icon.svg"
  },
  "permissions": [
    "storage",
    "clipboardRead",
    "*://www.copart.com/*"
  ],
  "background": {
    "scripts": [
      "browser-polyfill.js",
      "background.js",
      "download.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "24": "icons/SS-24.bmp",
      "48": "icons/SS-48.bmp",
      "96": "icons/SS-96.bmp"
    }
  },
  "options_ui": {
    "page": "options.html"
  },
  "update_url": "https://benetherington.com/salvage-search/updates.json"
}