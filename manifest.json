{
  "description": "Opens salvage inventory sites in new tabs, searching for the copied VIN",
  "manifest_version": 2,
  "name": "Salvage Search",
  "version": "1.0",
  "homepage_url": "https://github.com/benetherington/salvage-search",
  "icons": {
    "48": "icons/SS-48.png"
  },
  "permissions": [
    "storage",
    "clipboardRead"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/icon.svg",
      "96": "icons/icon.svg"
    }
  },
  "options_ui": {
    "page": "options.html"
  }
}